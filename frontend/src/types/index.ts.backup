// Type definitions for ZamStay

// Property type
export interface Property {
    id: number;
    title: string;
    description: string;
    price_per_night: number;
    city: string;
    has_wifi: boolean;
    owner_id: number;
}

// User type
export interface User {
    id: number;
    email: string;
    role: "guest" | "partner" | "admin";
    name?: string;
}

// Booking type
export interface Booking {
    id: number;
    property_id: number;
    user_id: number;
    check_in: string;
    check_out: string;
    total_price: number;
    status: "pending" | "confirmed" | "cancelled";
}

// Re-export types properly
export type { Property, User, Booking };
